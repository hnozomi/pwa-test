mand: \"add\",\n      inputValue: inputValue,\n      inputCost: inputCost,\n      inputDate: inputDate,\n      inputCycle: inputCycle,\n      inputRemark: inputRemark\n    };\n\n    axios.get(URL, { params: param }).then((response) => {\n      setArraySubscription(response.data[0]);\n    });\n\n    onBackHome();\n  };\n\n  const onDeleteSubscription = (subscriptionID) => {\n    const URL =\n      \"https://f1j83dq2c6.execute-api.ap-northeast-1.amazonaws.com/default/Subscription\";\n\n    const param = {\n      command: \"del\",\n      subscriptionID: subscriptionID\n    };\n\n    axios.get(URL, { params: param }).then((response) => {\n      setArraySubscription(response.data[0]);\n    });\n\n    onBackHome();\n  };\n\n  const onChangeUser = (e) => {\n    setChangeUser(e.target.value);\n  };\n\n  // user: true ⇨ 表示するテーブルIDセット完了。false ⇨ セット未完了\n  // loading: true ⇨ データ取得完了。false ⇨ データ取得未完了\n  // subscription: true ⇨ ホーム画面(データ取得完了)。false ⇨ ホーム画面以外\n  // detail: true ⇨ 詳細画面。false ⇨ 詳細画面以外\n\n  return (\n    <>\n      <div className=\"main-header\">\n        <header onClick={onBackHome} className=\"main-header-text\">\n          サブスク管理アプリ\n        </header>\n        <button onClick={onDisplayForm} className=\"main-header-add\">\n          <img alt=\"追加アイコン\" src=\"/AddIcon.png\" />\n          {/* <img alt=\"追加アイコン\" src=\"AddSubscription.svg\" /> */}\n        </button>\n      </div>\n      {table || (\n        <Dimmer active>\n          <Form>\n            <Form.Field>\n              <label>ユーザーID</label>\n              <Input\n                className=\"add-form-input\"\n                placeholder=\"サブスク名\"\n                onChange={onChangeUser}\n              />\n            </Form.Field>\n            <Button onClick={onClickGetSubscription} type=\"submit\">\n              Submit\n            </Button>\n          </Form>\n        </Dimmer>\n      )}\n      {/* &&は、左がtrueの、右側を返す */}\n      {/* ||は、左がfalseの、右側を返す */}\n      {loading && (\n        <Dimmer active>\n          <Loader>Loading</Loader>\n        </Dimmer>\n      )}\n      {subscription && (\n        <Home\n          onClickSubscription={onClickSubscription}\n          arraySubscription={arraySubscription}\n          arrayTotalCost={arrayTotalCost}\n        ></Home>\n      )}\n      {/* {subscription && (\n        <Home\n          onClickSubscription={onClickSubscription}\n          arraySubscription={arraySubscription}\n          arrayTotalCost={arrayTotalCost}\n        ></Home>\n      )} */}\n      {detail && (\n        <Detail\n          arraySubscription={arraySubscription}\n          arrayDetail={arrayDetail}\n          onDeleteSubscription={onDeleteSubscription}\n        ></Detail>\n      )}\n      {addsubscription && (\n        <Register\n          onBackHome={onBackHome}\n          onAddSubscription={onAddSubscription}\n        ></Register>\n      )}\n    </>\n  );\n};\n","C:\\Users\\ak62_\\OneDrive\\添付ファイル\\デスクトップ\\pwa-test\\src\\components\\Register.jsx",[],"C:\\Users\\ak62_\\OneDrive\\添付ファイル\\デスクトップ\\pwa-test\\src\\index.js",[],"C:\\Users\\ak62_\\OneDrive\\添付ファイル\\デスクトップ\\pwa-test\\src\\components\\Detail.jsx",["30"],"C:\\Users\\ak62_\\OneDrive\\添付ファイル\\デスクトップ\\pwa-test\\src\\components\\Home.jsx",["31","32"],{"ruleId":"33","severity":1,"message":"34","line":1,"column":27,"nodeType":"35","messageId":"36","endLine":1,"endColumn":36},{"ruleId":"33","severity":1,"message":"37","line":16,"column":10,"nodeType":"35","messageId":"36","endLine":16,"endColumn":14},{"ruleId":"33","severity":1,"message":"38","line":20,"column":9,"nodeType":"35","messageId":"36","endLine":20,"endColumn":27},{"ruleId":"33","severity":1,"message":"39","line":2,"column":27,"nodeType":"35","messageId":"36","endLine":2,"endColumn":31},{"ruleId":"33","severity":1,"message":"40","line":1,"column":17,"nodeType":"35","messageId":"36","endLine":1,"endColumn":25},{"ruleId":"33","severity":1,"message":"41","line":3,"column":35,"nodeType":"35","messageId":"36","endLine":3,"endColumn":42},"no-unuse